{
    "name": "lofty-dash-board",
    "version": "1.0.0",
    "description": "lofty dashboard project",
    "main": "index.js",
    "repository": "https://git.coding.net/locotor/Lofty.git",
    "scripts": {
        "build:dll": "npm run webpack -- --config config/webpack.dll.js -p",
        "build:dev": "npm run webpack -- --config config/webpack.dev.js --progress --profile",
        "build:prod": "npm run clean:dist && npm run webpack -- --config config/webpack.prod.js  --optimize-minimize  --progress --profile --bail",
        "build": "npm run build:prod",
        "clean:dist": "rimraf -- dist",
        "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
        "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --progress --profile --watch --content-base src/",
        "server:prod": "http-server dist -c-1 --cors",
        "start": "npm run server:dev",
        "watch:dev": "npm run build:dev -- --watch",
        "watch:prod": "npm run build:prod -- --watch",
        "watch": "npm run watch:dev",
        "node": "node",
        "rimraf": "rimraf",
        "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
    },
    "author": "locotor",
    "license": "ISC",
    "dependencies": {
        "@angular/animations": "^5.2.0",
        "@angular/cdk": "^5.2.0",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/platform-server": "^5.2.0",
        "@angular/router": "^5.2.0",
        "core-js": "^2.5.3",
        "echarts": "^3.8.5",
        "font-awesome": "^4.7.0",
        "hoek": "^5.0.3",
        "http-server": "^0.11.1",
        "ie-shim": "^0.1.0",
        "jquery": "^3.2.1",
        "lodash-es": "^4.17.4",
        "moment": "^2.20.1",
        "ng-zorro-antd": "^0.7.0-beta.5",
        "ng2-file-upload": "^1.3.0",
        "ngx-quill-editor": "^2.2.2",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.20"
    },
    "devDependencies": {
        "@types/echarts": "0.0.9",
        "@types/jquery": "^3.2.17",
        "@types/node": "^9.3.0",
        "@types/webpack": "^3.8.2",
        "angular-router-loader": "^0.8.2",
        "angular2-template-loader": "^0.6.2",
        "assets-webpack-plugin": "^3.5.1",
        "autodll-webpack-plugin": "^0.3.8",
        "awesome-typescript-loader": "^3.4.1",
        "copy-webpack-plugin": "^4.3.1",
        "css-loader": "^0.28.8",
        "expose-loader": "^0.7.4",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.6",
        "html-webpack-plugin": "^2.29.0",
        "json-loader": "^0.5.7",
        "less": "^3.0.4",
        "less-loader": "^4.1.0",
        "node-sass": "^4.7.2",
        "optimize-js-plugin": "0.0.4",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "script-ext-html-webpack-plugin": "^1.8.8",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.19.1",
        "to-string-loader": "^1.1.5",
        "tslint": "^5.9.1",
        "typescript": "^2.6.2",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.10.1",
        "webpack-merge": "^4.1.1"
    }
}
